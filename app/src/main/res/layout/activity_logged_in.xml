<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoggedInActivity">

    <TextView
        android:id="@+id/logged_in_text_username"
        android:layout_width="152dp"
        android:layout_height="20dp"
        android:layout_marginEnd="80dp"
        app:layout_constraintBottom_toTopOf="@+id/logged_in_button_logout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logged_in_text_userid"
        app:layout_constraintVertical_bias="0.484" />

    <TextView
        android:id="@+id/logged_in_text_username_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Username: "
        app:layout_constraintBottom_toTopOf="@+id/logged_in_button_logout"
        app:layout_constraintEnd_toStartOf="@+id/logged_in_text_username"
        app:layout_constraintHorizontal_bias="0.583"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logged_in_text_userid_label"
        app:layout_constraintVertical_bias="0.485" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="167dp"
        android:layout_height="33dp"
        android:layout_marginTop="100dp"
        android:text="You are logged in!"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/logged_in_button_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="320dp"
        android:text="Logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.451"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/logged_in_text_userid_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UserID: "
        app:layout_constraintBottom_toTopOf="@+id/logged_in_text_username_label"
        app:layout_constraintEnd_toStartOf="@+id/logged_in_text_userid"
        app:layout_constraintHorizontal_bias="0.561"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/logged_in_text_userid"
        android:layout_width="152dp"
        android:layout_height="20dp"
        android:layout_marginEnd="80dp"
        app:layout_constraintBottom_toTopOf="@+id/logged_in_text_username"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/logged_in_button_interests"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="51dp"
        android:text="Settings"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.452"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logged_in_button_logout"
        app:layout_constraintVertical_bias="0.162" />
</androidx.constraintlayout.widget.ConstraintLayout>